{
    "name": "index-io/google",
    "description": "This is the Index custom library for interacting with Google gmail and calendar api.",
    "keywords": [
        "index",
        "google",
        "gmail",
        "google calendar"
    ],
    "homepage": "https://github.com/grigorecezar/google",
    "license": "MIT",
    "authors": [
        {
            "name": "Cezar Grigore",
            "email": "cezar@index.io",
            "homepage": "https://github.com/grigorecezar",
            "role": "Developer"
        },
        {
            "name": "Liviu Nasoi",
            "email": "liviu@index.io",
            "homepage": "https://github.com/Liviun92",
            "role": "Developer"
        },
        {
            "name": "Tom Jenkins",
            "email": "tom@index.io",
            "homepage": "https://github.com/tomtwo",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0.0",
        "google/apiclient": "2.*",
        "google/cloud": "^0.20.1",
        "larapack/dd": "1.*",
        "nesbot/carbon": "1.21.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "IndexIO\\Google\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IndexIO\\Google\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
